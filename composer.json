{
    "name": "javaabu/mobile-verification",
    "description": "Adds mobile number verification to Laravel projects.",
    "homepage": "https://github.com/Javaabu/mobile-verification",
    "license": "MIT",
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "javaabu/sms-notifications": "^1.0",
        "biscolab/laravel-recaptcha": "^6.1",
        "javaabu/helpers": "^1.30",
        "javaabu/activitylog": "^1.2",
        "laravel/passport": "^12.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5",
        "laravel/pint": "^1.15",
        "javaabu/auth": "^1.7"
    },
    "suggest": {
        "javaabu/auth": "Required for login and registration"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\MobileVerification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\MobileVerification\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\MobileVerification\\MobileVerificationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
